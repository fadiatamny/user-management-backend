{
    "entryFile": "src/app.ts",
    "noImplicitAdditionalProperties": "ignore",
    "controllerPathGlobs": ["src/controllers/*.controller.ts"],
    "spec": {
        "outputDirectory": "public",
        "specVersion": 3
    },
    "routes": {
      "routesDir": "src",
      "middleware": "express",
      "entryFile": "./src/controllers/index.ts",
      "basePath": "/api/"
    }
}